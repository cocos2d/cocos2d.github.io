<span class='line'>127</span> </span><span class="WHIT">        </span><span class="NAME">this._dimensions</span><span class="WHIT"> </span><span class="PUNC">=</span><span class="WHIT"> </span><span class="NAME">cc.size</span><span class="PUNC">(</span><span class="NAME">dimensions.width</span><span class="PUNC">,</span><span class="WHIT"> </span><span class="NAME">dimensions.height</span><span class="PUNC">)</span><span class="PUNC">;</span><span class="WHIT">
<span class='line'>128</span> </span><span class="WHIT">        </span><span class="NAME">this._fontName</span><span class="WHIT"> </span><span class="PUNC">=</span><span class="WHIT"> </span><span class="NAME">fontName</span><span class="WHIT"> </span><span class="PUNC">||</span><span class="WHIT"> </span><span class="STRN">"Arial"</span><span class="PUNC">;</span><span class="WHIT">
