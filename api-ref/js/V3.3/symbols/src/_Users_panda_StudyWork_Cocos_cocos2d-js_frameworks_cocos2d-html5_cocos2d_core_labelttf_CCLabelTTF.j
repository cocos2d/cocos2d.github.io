<span class='line'> 94</span> </span><span class="WHIT">    </span><span class="NAME">_lineWidths</span><span class="PUNC">:</span><span class="WHIT"> </span><span class="KEYW">null</span><span class="PUNC">,</span><span class="WHIT">
<span class='line'> 95</span> </span><span class="WHIT">    </span><span class="NAME">_className</span><span class="PUNC">:</span><span class="WHIT"> </span><span class="STRN">"LabelTTF"</span><span class="PUNC">,</span><span class="WHIT">
